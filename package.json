    "babel-jest": "29.6.3",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.19.0",
    "jest": "29.6.3",
    "lefthook": "1.6.12",
    "patch-package": "8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
